{
  "name": "assman",
  "version": "1.0.0",
  "description": "This is created to allow a master machine to execute any type of command on the slave machines.",
  "main": "index.js",
  "scripts": {
    "master:server": "nodemon --delay 1s --watch src/master/server/ src/common/ --exec \"babel-node src/master/server/index.js\"",
    "slave": "nodemon --delay 1s --watch src/slave/ /src/common/ --exec \"babel-node src/slave/index.js\"",
    "test": "mocha --require @babel/register"
  },
  "imports": {
    "#src/*": "./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datnguyen1215/assman.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/datnguyen1215/assman/issues"
  },
  "homepage": "https://github.com/datnguyen1215/assman#readme",
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.22.5",
    "@babel/node": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/register": "^7.22.5",
    "chai": "^4.3.7",
    "jscommon": "github:datnguyen1215/jscommon",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3"
  }
}
